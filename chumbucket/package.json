  "author": "stagfoo",
  "license": "ISC",
  "pre-commit": [
    "lint:fix",
    "spellcheck"
  ],
  "dependencies": {
    "combokeys": "^3.0.1",
    "joro": "^1.0.4",
    "keyboard-handler": "^2.0.0",
    "morphdom": "^2.6.1",
    "nanohtml": "^1.9.0",
    "nanomorph": "^5.4.3",
    "nid": "^2.0.0",
    "obake.js": "^1.2.1",
    "page": "^1.11.4"
  },
  "devDependencies": {
    "@types/feather-icons": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "cspell": "^5.18.5",
    "eslint": "^8.9.0",
    "eslint-config-xo": "^0.40.0",
    "pre-commit": "^1.2.2",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.9.10",
    "webpack": "^4.43.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "5.0.0"
  }
